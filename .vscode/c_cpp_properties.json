{
  "env": {
    "myDefaultIncludePath": [
        "${workspaceFolder}",
        // "${workspaceFolder}/core/common/inc",
    ],
    "myCompilerPath": "/usr/bin/g++"
  },
  "configurations": [
    {
      "name": "ROS",
      "intelliSenseMode": "gcc-x64",
      "includePath": ["${myDefaultIncludePath}"],
      "compilerPath": "/usr/bin/g++",
      "cStandard": "c11",
      "cppStandard": "c++17",
    //   "compileCommands": "/root/catkin_ws/build/compile_commands.json",
      "browse": {
        "path": ["${workspaceFolder}"],
        "limitSymbolsToIncludedHeaders": true,
        "databaseFilename": ""
      }
    }
  ],
  "version": 4
}
